@model ZRui.Web.CommunityArgsModel
@{
    ViewBag.Title = "Detail";
    ViewBag.ngController = "xiechengIndexController";
    Layout = "~/Views/Shared/_LayoutMember.cshtml";
}
<section ng-init="init('@Model.CommunityFlag','@Model.AppFlag')">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            求职列表
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> 首页</a></li>
            @*<li><a href="#">Examples</a></li>*@
            <li class="active">求职列表</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <!-- Default box -->
        <div class="box">
            <div class="box-header with-border">
                <div>
                    <select select2 class="form-control input-sm" placeholder="请先选择职位" ng-model="searchModel.jobId" ng-change="load()" style="display: inline-block;width: 350px;">
                        <option value="{{ item.jobId }}" ng-repeat="item in jobRecList">{{ item.jobTitle }}({{ item.jobCode }})</option>
                    </select>
                    <!--<button class="btn" ng-click="goSetPending()">待定</button>-->
                    <button class="btn btn-sm" ng-click="goFail()">淘汰</button>
                    <button class="btn btn-sm" ng-click="goPass()">下一阶段</button>
                    <button class="btn btn-sm" ng-click="goSetPhase()" ng-if="searchModel.jobId">批量转移</button>
                    <button class="btn btn-sm" ng-click="printMore()" ng-if="searchModel.jobId">批量打印</button>
                    <button class="btn btn-sm" ng-click="sendMesByInput()" ng-if="searchModel.jobId">发送消息</button>
                    <button class="btn btn-sm" ng-click="sendMailByInput()" ng-if="searchModel.jobId">转发资料</button>
                </div>

                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-warning" ng-click="exportModelOpen()">导出</button>
                </div>
            </div>
            <div class="box-body table-responsive no-padding">
                <table class="table colResizable table-hover" style="width:98%;">
                    <thead>
                        <tr>
                            <th width='70' style="text-align:center"><input type="checkbox" ng-checked="selectAll" name="" ng-click="selectValAll()"></th>
                            <th style="text-align:center">姓名</th>
                            <th style="text-align:center">资料完整度</th>
                            <th style="text-align:center">岗位</th>
                            <th style="text-align:center">阶段</th>
                            <th style="text-align:center">状态</th>
                            <th style="text-align:center">招聘状态</th>
                            <th style="text-align:center">工具栏</th>
                        </tr>
                    </thead>
                    <tr>
                        <td style="text-align:center;">---</td>
                        <td><input type="text" class="form-control input-sm" ng-model="searchModel.personName" ng-change="load()" ng-model-options="{debounce:1000}" esc-key="resetFilter()" /></td>
                        <td style="text-align:center;">---</td>
                        <td style="text-align:center;">--</td>
                        <td style="text-align:center;">
                            <select class="form-control input-sm" ng-model="searchModel.phaseName" ng-options="item.value as item.name for item in searchPhaseList" ng-change="load()"></select>
                        </td>
                        <td style="text-align:center;">
                            <select class="form-control input-sm" ng-model="searchModel.statusName" ng-options="item.value as item.name  for item in searchStatusList" ng-change="load()"></select>
                        </td>
                        <td style="text-align:center;">
                            <select class="form-control input-sm" ng-model="searchModel.jobRecruitmentStatus" ng-options="item.value as item.name  for item in searchJobStatusList" ng-change="load()"></select>
                        </td>
                        <td style="text-align:center;">---</td>
                    </tr>
                    <tr ng-repeat="item in items">
                        <td style="text-align:center;padding-top:0;padding-bottom:0;"><label for="{{item.id}}" style="display: block;padding-top: 8px;margin-bottom: 0;padding-bottom: 8px;cursor: pointer;"><input type="checkbox" id="{{item.id}}" name="idss" ng-checked="isSelected(item.id)" ng-click="selectVal($event,item.id,$index)"></label></td>
                        <td style="text-align:center;cursor:pointer" ng-click="edit($index)">{{item.personName}}</td>
                        <td style="text-align:center;">{{item.completeness}}%</td>
                        <td style="text-align:center;">{{item.jobName}}</td>
                        <td style="text-align:center;">{{item.phaseName}}</td>
                        <td style="text-align:center;">{{item.statusName}}</td>
                        <td style="text-align:center">{{item.jobRecruitmentStatus}}</td>
                        <td style="text-align:center"><a href="javascript:;" ng-click="startChat(isBindWX[item.applicantId],item.personName)" ng-show="isBindWX[item.applicantId]"><i class="fa fa fa-comments" style="font-size:18px;"></i></a> <!--<a href="javascript:;" ng-click="myPrint(item.applicantId)"><i class="fa fa-print" style="font-size:18px;"></i></a> --> <a href="javascript:;" ng-click="goSetPhase(item.id,$index)"><i class="fa fa-random" style="font-size:18px;"></i></a> <a href="javascript:;" ng-click="sendMesByInput(item.id,$index)" ng-show="isBindWX[item.applicantId]"><i class="fa fa-bell" style="font-size:18px;"></i></a></td>
                    </tr>
                </table>
            </div>
            <!-- /.box-body -->
            <div class="box-footer">
                <ul  ng-show="totalCount>pageSize" uib-pagination total-items="totalCount" items-per-page="pageSize" ng-model="pageIndex" max-size="6" class="pagination-sm" boundary-link-numbers="true" rotate="false" ng-change="load()"></ul>
            </div>
        </div>
        <!-- /.box -->
    </section>
    <!-- /.content -->

<!-- 模态框（Modal） -->
<div class="modal fade bs-example-modal-sm" id="errorModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" style="z-index: 9999;">
  <div class="modal-dialog modal-sm" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
					&times;
				</button>
				<h4 class="modal-title" id="myModalLabel">
					消息提示
				</h4>
			</div>
			<div class="modal-body">
				{{addError}}
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal">关闭
				</button>
			</div>
		</div><!-- /.modal-content -->
	</div><!-- /.modal -->
</div>

<!-- 导出 模态框（Modal） -->
<div class="modal fade bs-example-modal-sm" id="exportModal" tabindex="-1" role="dialog" style="z-index: 9999;">
  <div class="modal-dialog modal-sm" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal" aria-hidden="true">
					&times;
				</button>
				<h4 class="modal-title">
					批量导出
				</h4>
			</div>
			<div class="modal-body">
				<select class="form-control" ng-model="export.type" ng-change="exportSource()" style="width:213px;margin-left:10px;">
                    <option value="all">导出全部</option>
                    <option value="page">导出本页</option>
                </select>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-warning" add-bom="true" csv-header="['编号', '姓名', '岗位', '阶段', '状态']" ng-csv="export.data" filename="export.csv">确定导出</button>
				<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
			</div>
		</div><!-- /.modal-content -->
	</div><!-- /.modal -->
</div>

    <!-- Modal -->
<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">转移到</h4>
      </div>
      <div class="modal-body">
          <form class="form-inline">
          <div class="form-group" style="margin-bottom:10px;display:block;">
            <label for="Interviewer">阶段</label>
            <!-- <select class="form-control" ng-model="newModel.phaseName" ng-change="statusChangeByProcess()" style="width:213px;margin-left:10px;">
                <option value="{{item}}" ng-repeat="item in processPhasesSet">{{item}}</option>
            </select> -->
            <span ng-repeat="item in processPhasesSet" class="processBtn processArraw" ng-class="{'active':item==newModel.phaseName}" ng-click="statusChangeByProcess(item)">{{item}}</span>
          </div>
          <div class="form-group" style="margin-bottom:10px;margin-top:20px;display:block;">
            <label for="Interviewer">状态</label>
            <!-- <select class="form-control" ng-model="newModel.statusName" style="width:213px;margin-left:10px;">
                <option value="{{item}}" ng-repeat="item in statusPhasesSet">{{item}}</option>
            </select> -->
            <span ng-repeat="item in statusPhasesSet" class="processBtn" ng-class="{'active':item==newModel.statusName}" ng-click="statusSelect(item)">{{item}}</span>
          </div>
          </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-block btn-primary" ng-click="goSetPhaseOK()">提交</button>
      </div>
    </div>
  </div>
</div>

    <!-- 手动发送模板消息 -->
<div class="modal fade" id="mesModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">发送模板消息</h4>
      </div>
      <div class="modal-body">
          <form class="form-inline">
          <div class="form-group" style="margin-bottom:10px">
            <label for="Interviewer">模板</label>
            <select class="form-control" ng-model="mesByInput.selectIndex" style="width:213px;margin-left:10px;">
                <option value="-1">-- 请选择 --</option>
                <option value="{{item.index}}" ng-repeat="item in mesByInput.template">{{item.text}}</option>
            </select>
          </div>
          <div ng-if="mesByInput.selectIndex>-1">
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">导语(first)</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].first" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">{{mesByInput.data[mesByInput.selectIndex].txt1}}</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].keyword1" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">{{mesByInput.data[mesByInput.selectIndex].txt2}}</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].keyword2" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">{{mesByInput.data[mesByInput.selectIndex].txt3}}</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].keyword3" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">备注(remark)</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].remark" type="text" style="width:100%;" />
            </div>
          </div>
          </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-block btn-primary" ng-click="sendMesByInputSubmit()" ng-if="mesByInput.selectIndex>-1">发送消息</button>
      </div>
    </div>
  </div>
</div>


    <!-- 手动发送模板消息(在对话框调起发送) -->
