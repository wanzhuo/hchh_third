            </select>
          </div>
          <div ng-if="mesByInput.selectIndex>-1">
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">导语(first)</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].first" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">{{mesByInput.data[mesByInput.selectIndex].txt1}}</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].keyword1" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">{{mesByInput.data[mesByInput.selectIndex].txt2}}</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].keyword2" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px" ng-if="mesByInput.data[mesByInput.selectIndex].txt3">
                <label for="Interviewer">{{mesByInput.data[mesByInput.selectIndex].txt3}}</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].keyword3" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px" ng-if="mesByInput.data[mesByInput.selectIndex].txt4">
                <label for="Interviewer">{{mesByInput.data[mesByInput.selectIndex].txt4}}</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].keyword4" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">备注(remark)</label>
                <input class="form-control" ng-model="mesByInput.data[mesByInput.selectIndex].remark" type="text" style="width:100%;" />
            </div>
          </div>
          </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-block btn-primary" ng-click="sendMesByInputSubmitOnChat()">发送消息</button>
      </div>
    </div>
  </div>
</div>

  <!-- 手动发送邮件 -->
<div class="modal fade" id="mailModal" tabindex="-1" role="dialog">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">转发资料</h4>
      </div>
      <div class="modal-body">
          <form class="form-inline" name="myForm">
          <div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">收件人Email</label>
                <input class="form-control" placeholder="请填写Email" name="myAddress" ng-model="sendMailData.to" type="email" autocomplete="true" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">收件人称呼</label>
                <input class="form-control" ng-model="sendMailData.name" type="text" style="width:100%;" />
            </div>
            <div class="" style="margin-bottom:10px">
                <label for="Interviewer">邮件标题</label>
                <input class="form-control" ng-model="sendMailData.title" type="text" style="width:100%;" />
            </div>
          </div>
          </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-block btn-primary" ng-disabled="myForm.myAddress.$error.email || !sendMailData.to || sendMailData.isSubmit" ng-click="sendMailByInputSubmit()">发送邮件</button>
      </div>
    </div>
  </div>
</div>

